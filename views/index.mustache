{{#gabs}}
<div class="gabContainer">
  <div class="insideContainer">
    <p><span class="createdBy">{{#createdBy}}{{username}}{{/createdBy}}</span>:  {{body}}</p>
    <p class="createdAt">{{createdAt}}</p>
    <span class="msgId">{{id}}</span>
    <a href="/message/{{id}}"><span class="likes">{{likedBy.length}}</span></a>
  </div>
  <div class="buttonContainer">
    <form class="likeBtnForm" action="likeMsg" method="post">
      <button name="id" class="likeBtn" value="{{id}}">Like</button>
    </form>
    <form class="delBtnForm" action="/delMsg" method="post">
      <button name="id" class="delBtn" value="{{id}}">Delete</button>
    </form>
  </div>
</div>

<hr class="sexy-line">
{{/gabs}}


<script type="text/javascript" src="/public/js/index.js">

</script>
